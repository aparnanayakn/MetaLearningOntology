    <!-- https://purl.archive.org/domain/mtl#Assortativity -->

    <owl:Class rdf:about="https://purl.archive.org/domain/mtl#Assortativity">
        <rdfs:subClassOf rdf:resource="http://www.e-LICO.eu/ontologies/dmo/DMOP/DMOP.owl#DataSetCharacteristic"/>
